 
    <div class="container-fluid">
      <div class="row">
         <div class="col-md-12">
           <div class="well">
              <h2>Contact Us</h2>
              {{#if errors}}
                  {{#each errors}}
                     <div class="alert alert-danger">{{msg}} </div>
                  {{/each}}
              {{/if}}
              <form action="contact" method="post">
                  <div class="form-group">
                     <label>Name</label>
                     <input type="text" class="form-control" name="name" placeholder='Name' required>
                  </div>
                  <div class="form-group">
                     <label>Email address</label>
                     <input type="email" class="form-control" name="email" placeholder='email' required>
                  </div>
                  <div class="form-group">
                     <label>Message</label>
                     <textarea class="form-control"  name="message"  placeholder='message' required></textarea>
                  </div>
                  <button type="submit" class="btn btn-default">Submit</button>
               </form>
               <br><br>
                <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d101071.38485273147!2d-121.94774307047626!3d37.661409293719046!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x808fe9a261ba755f%3A0xb3ab6847e1ea7d16!2sPleasanton%2C+CA!5e0!3m2!1sen!2sus!4v1486526249489" width="700" height="450" frameborder="0" style="border:0"></iframe>
            </div>  <!-- well -->
         </div> <!-- col -->
      </div> <!-- row -->
    </div> <!-- /container -->